<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VaccinePortal"
    android:orientation="vertical"
    android:background="@color/back">


    <TextView
        android:id="@+id/textVaccine"
        android:layout_width="match_parent"
        android:layout_height="61dp"
        android:layout_gravity="center"
        android:fontFamily="@font/rubikbold"
        android:padding="20dp"
        android:text="Click HERE to open the Vaccine Portal"
        android:textColor="@color/topbar"
        android:textSize="19dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.829"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="308dp"
        android:layout_height="65dp"
        android:fontFamily="@string/regular"
        android:text="Welcome To The ICMR Vaccine Portal"
        android:textColor="@color/white2"
        android:textSize="20dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/textVaccine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.184" />

    <TextView
        android:padding="5dp"
        android:id="@+id/textView5"
        android:layout_width="288dp"
        android:layout_height="365dp"
        android:textColor="@color/white2"
        android:textSize="16dp"
        android:text="@string/vaccine"
        android:gravity="start"
        app:layout_constraintBottom_toTopOf="@+id/textVaccine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.419" />

</androidx.constraintlayout.widget.ConstraintLayout>